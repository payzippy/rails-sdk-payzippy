<html>
<head>
  <style>
      .hash_match_success {
          color: green;
      }

      .hash_match_failed {
          color: red;
      }
  </style>
  <%= stylesheet_link_tag "bootstrap" %>
  <title>Payzippy Integration Examples</title>
  <link href="dirname(__FILE__)/../../css/bootstrap.css" rel="stylesheet" type="text/css">
  <style>
      .required {
          font-weight: bold;
      }
  </style>
</head>
<body>
<div class="navbar navbar-inverse">
  <div class="navbar-inner">
    <div class="container">
      <a class="brand" href="../../">Rails SDK</a>

      <div class="nav-collapse collapse">
        <ul class="nav">
          <li><a href="../charging/show">Charging</a></li>
          <li><a href="../query/show">Query</a></li>
          <li><a href="../refund/show">Refund</a></li>
        </ul>
      </div>
      <!--/.nav-collapse -->
    </div>
  </div>
</div>

<% if @is_hash_match %>
    <div class="hash_match_success">
      <h3>Hash Matching Successful</h3></br>
    </div>
<% else %>
    <div class="hash_match_failed">
      <h3>Hash Matching Failed</h3></br>
    </div>
<% end %>


<br><br>
Status Code: <%= @status_code %>
<br/>
Status Message: <%= @status_message %></br>
Error Code: <%= @error_code %></br>
Error Message: <%= @error_message %></br>
Merchant ID: <%= @merchant_id %></br>
Merchant Key ID: <%= @merchant_key_id %></br>
Hash Method: <%= @hash_method %></br>
Hash Calculated: <%= @hash_calc %></br>
Hash Received: <%= @hash %></br>
<br>
<br/>
</br>
<h3>Query Data:</h3><br>
<% @data_params["data"].each do |num| %>
    <% num.each do |key, val| %>
        <%= key.gsub("_", " ").titleize %>:&nbsp;
        <%= val %></br></hr>
    <% end %>
    </br>
    <hr>
<% end %>

</body>
</html>